<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.aplikasistokbarang">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiStokBarang">
        <activity
            android:label="@string/app_name_edit"
            android:name=".EditBarang"
            android:parentActivityName=".ViewDaftarBarang"/>
        <activity
            android:name=".TambahData"
            android:label="@string/app_name_tambah"
            android:parentActivityName=".Home_Activity"/>
        <activity
            android:name=".ViewDaftarBarang"
            android:parentActivityName=".Home_Activity"/>
        <activity
            android:label="@string/app_name_main"
            android:name=".Home_Activity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>